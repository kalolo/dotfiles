; .editorconfig
root = true
[*]
indent_style = space
indent_size = 2
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true
[**.json]
indent_size = 4
[*.md]
insert_final_newline = false
trim_trailing_whitespace = false
[**.sql]
indent_style = space
[**.yml]
indent_style = space
[Makefile]
indent_style = tab
